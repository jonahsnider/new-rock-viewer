		"bentocache": "^1.5.0",
		"cheerio": "^1.1.2",
		"envalid": "^8.1.1",
		"normalize-url": "^8.1.0",
		"path-exists": "^5.0.0",
		"playwright": "^1.56.1",
		"playwright-extra": "^4.3.6",
		"puppeteer-extra-plugin-stealth": "^2.11.2",
		"react": "^19.1",
		"react-dom": "^19.1",
		"sanity": "^4.19.0",
		"styled-components": "^6.1.18",
		"zod": "^4.1.13"
