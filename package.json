	"private": true,
	"keywords": [
		"sanity"
	],
	"license": "UNLICENSED",
	"type": "module",
	"main": "package.json",
	"scripts": {
		"build": "tsc",
		"dev": "node --watch --experimental-transform-types --env-file-if-exists=.env src/index.ts",
		"lint": "biome check",
		"lint:fix": "biome check --fix --unsafe",
		"sanity:build": "sanity build",
		"sanity:deploy": "sanity deploy",
		"sanity:deploy-graphql": "sanity graphql deploy",
		"sanity:dev": "sanity dev",
		"sanity:start": "sanity start",
		"sanity:typegen": "sanity schema extract --enforce-required-fields && sanity typegen generate",
		"start": "node --experimental-transform-types --env-file-if-exists=.env src/index.ts"
	},
	"dependencies": {
		"@clack/prompts": "^1.0.0-alpha.7",
